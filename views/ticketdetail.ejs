<!DOCTYPE html>
<html lang="en">

<head>
  <title>Ticket Detail</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
  <link rel="stylesheet" href="https://www.w3schools.com/lib/w3-theme-black.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css">
</head>

<body>

  <div id="nav"></div>
  <div id="header"></div>
  
  <h4>Hello <%= user.username %>
  </h4>

  <h1>Ticket Detail</h1>

  <table id="table" style="background-color:rgb(226, 228, 146);">
    <thead>
        <th>Ticket Id</th>
        <th>Train Number</th>
        <th>Train Name</th>
        <th>Source</th>
        <th>Source Arrival</th>
        <th>Source Departure</th>
        <th>Destination</th>
        <th>Destination Arrival</th>
        <th>Destination Departure</th>
        <th>Coach Number</th>
        <th>Seat Number</th>
        <th>Seat Type</th>
        <th>Distance Journey</th>
        <th>Fare</th>
    </thead>
    <thead>
        <th id="ticket-id"></th>
        <th id="train-num"></th>
        <th id="train-name"></th>
        <th id="src"></th>
        <th id="src-arr"></th>
        <th id="src-dep"></th>
        <th id="dest"></th>
        <th id="dest-arr"></th>
        <th id="dest-dep"></th>
        <th id="coach-num"></th>
        <th id="seat-num"></th>
        <th id="seat-type"></th>
        <th id="distance"></th>
        <th id="fare"></th>
    </thead>
    <tbody></tbody>
  </table>

  <div id="dynamic_list"></div>

  <div id="footer"></div>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="/scripts/template.js"></script>
  <script src="/scripts/ticketsDetail.js"></script>

</body>

</html>